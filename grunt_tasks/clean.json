{
  "cache": {
    "files": {
        "dot": "true",
        "src": ["<%= symfony.cache %>/*",".sass-cache"]}
  },

  "dist": {
    "files": {
      "src": ["<%= dist.root %>"]
    }
  },

  "build": {
    "files": {
      "src": ["<%= dist.root %>/composer.*", 
              "<%= dist.root %>/*.md",
              "<%= dist.cache %>/*",
              "<%= dist.logs %>/*",
              "<%= dist.root %>/**/sass",
              "<%= dist.root %>/**/sprites*/"]
    }
  }
}